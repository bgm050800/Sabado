@model List<SM_WEB.Entities.Usuario>

@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-hover">
    <thead>
        <tr>
            <th>Identificación</th>
            <th>Nombre</th>
            <th>Correo</th>
            <th>Estado</th>
        </tr>
    </thead>
    <tbody>
      @foreach (var item in Model)
        {
            <tr>
                <td>@item.Identificacion</td>
                <td>@item.Nombre</td>
                <td>@item.Correo</td>
                <td>@item.Estado</td>
            </tr>
        }
    </tbody>
</table>